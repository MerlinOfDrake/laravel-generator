{
    "name": "bluecode/laravel-generator",
    "description": "Laravel Generator",
    "keywords": [
        "laravel",
        "generator",
        "model",
        "migration",
        "scaffold",
        "CRUD",
        "MVC"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bluecode"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "laravelcollective/html":"5.*"
    },
    "autoload": {
        "psr-4": {
            "Bluecode\\Generator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bluecode\\Generator\\GeneratorServiceProvider"
            ]
        }
    }
}
